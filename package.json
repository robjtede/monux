{
  "name": "Monux",
  "version": "0.9.3",
  "description": "Unoffical Monzo Client for Desktop",
  "private": true,
  "main": "src/app.ts",
  "repository": "https://github.com/robjtede/monux",
  "author": "robjtede",
  "license": "MIT",
  "scripts": {
    "start": "webpack --watch",
    "ng": "ng",
    "lint": "ng lint",
    "webpack": "webpack",
    "build": "webpack --display-error-details && npm run build:electron",
    "build:prod": "cross-env NODE_ENV=production npm run build",
    "build:electron:tsc": "tsc ./src/app.ts --outDir ./dist --target es2015 --module commonjs --skipLibCheck",
    "build:electron": "npm run build:electron:tsc",
    "electron:serve": "npm run build:electron && npm run electron:test -- --serve",
    "electron:test": "electron ./dist/app.js",
    "electron:dev": "npm run build && npm run electron:test",
    "electron:prod": "npm run build:prod && npm run electron:test",
    "electron:linux": "npm run build:prod && node tools/package.js --platform=linux --arch=x64",
    "electron:windows": "npm run build:prod && node tools/package.js --platform=win32 --arch=ia32",
    "electron:mac": "npm run build:prod && node tools/package.js --platform=darwin --arch=x64",
    "test": "karma start ./karma.conf.js",
    "pree2e": "webdriver-manager update --standalone false --gecko false --quiet && npm run build",
    "e2e": "protractor ./protractor.conf.js",
    "precommit": "lint-staged"
  },
  "dependencies": {
    "@angular-redux/store": "^6.5.7",
    "@angular/common": "^4.3.4",
    "@angular/compiler": "^4.3.4",
    "@angular/core": "^4.3.4",
    "@angular/platform-browser": "^4.3.4",
    "@angular/platform-browser-dynamic": "^4.3.4",
    "@ngrx/store": "^4.0.2",
    "angular2-autosize": "^1.0.1",
    "common-tags": "^1.4.0",
    "d3": "^4.10.0",
    "date-fns": "^2.0.0-alpha.1",
    "debug": "^2.6.8",
    "dexie": "^2.0.0-beta.11",
    "electron": "^1.7.5",
    "electron-contextmenu-middleware": "^1.0.3",
    "electron-devtools-installer": "^2.2.0",
    "electron-image-menu": "^2.0.0",
    "electron-window-state": "^4.1.1",
    "img-exif": "^0.2.0",
    "keytar": "^4.0.4",
    "lodash": "^4.17.4",
    "p-iteration": "^1.1.5",
    "redux": "^3.7.2",
    "redux-actions": "^2.2.1",
    "redux-promise-middleware": "^4.3.0",
    "reflect-metadata": "^0.1.10",
    "request": "^2.81.0",
    "request-promise-native": "^1.0.4",
    "rxjs": "^5.4.3",
    "zone.js": "^0.8.16"
  },
  "devDependencies": {
    "@angular/cli": "^1.3.0",
    "@angular/compiler-cli": "^4.3.4",
    "@ngtools/webpack": "^1.6.0",
    "@types/common-tags": "^1.2.5",
    "@types/d3": "^4.10.0",
    "@types/debug": "0.0.29",
    "@types/electron-config": "^0.2.1",
    "@types/electron-devtools-installer": "^2.0.2",
    "@types/electron-window-state": "^2.0.29",
    "@types/keytar": "^4.0.0",
    "@types/lodash": "^4.14.72",
    "@types/node": "^8.0.19",
    "@types/redux-actions": "^1.2.7",
    "@types/redux-promise": "^0.5.24",
    "@types/request-promise-native": "^1.0.6",
    "angular2-template-loader": "^0.6.2",
    "awesome-typescript-loader": "^3.2.2",
    "copyfiles": "^1.2.0",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.4",
    "devtron": "^1.4.0",
    "eclint": "^2.3.0",
    "electron-packager": "^8.7.2",
    "electron-reload": "^1.2.1",
    "eslint": "^4.3.0",
    "eslint-config-standard": "^10.2.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-node": "^5.1.1",
    "eslint-plugin-promise": "^3.5.0",
    "eslint-plugin-standard": "^3.0.1",
    "extract-text-webpack-plugin": "^3.0.0",
    "file-loader": "^0.11.2",
    "html-loader": "^0.5.1",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "lint-staged": "^4.0.2",
    "postcss": "^6.0.9",
    "postcss-cssnext": "^3.0.2",
    "postcss-loader": "^2.0.6",
    "prettier": "^1.5.3",
    "raw-loader": "^0.5.1",
    "style-loader": "^0.18.2",
    "to-string-loader": "^1.1.5",
    "tslib": "^1.7.1",
    "tslint": "^5.4.3",
    "typescript": "^2.4.2",
    "webpack": "^3.5.3"
  },
  "lint-staged": {
    "!*.{js,ts,css}": [
      "eclint fix",
      "git add"
    ],
    "*.js": [
      "eclint fix",
      "prettier --write --no-semi --single-quote",
      "eslint --fix",
      "git add"
    ],
    "*.ts": [
      "eclint fix",
      "prettier --write --no-semi --single-quote",
      "git add"
    ],
    "*.css": [
      "eclint fix",
      "prettier --write",
      "git add"
    ]
  }
}
